{
  "expo": {
    "name": "My-Personal-Library-Front",
    "slug": "my-personal-library-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.jose-oliv.mypersonallibraryfront",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to scan barcodes."
      }
    },
    "android": {
      "permissions": [
        "CAMERA"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.joseoliv.mypersonallibraryfront"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "platforms": [
        "ios",
        "android",
        "web"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.958080376950-ov7dgq16sggjncpa7u5p4edesradrr0g"
        }
      ],
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "bb066308-1f75-49d3-afaa-b0e7d10de5b3"
      },
      "google": {
        "iosClientId": "958080376950-ov7dgq16sggjncpa7u5p4edesradrr0g.apps.googleusercontent.com"
      },
      "googleClientId": "958080376950-ii8vapnkoq7r9s7ji8ps55fh09ie47m7.apps.googleusercontent.com"
    },
    "owner": "jose_oliv",
    "newArchEnabled": true
  }
}
